{
  "name": "Bee - CopounFinder",
  "version": "1.0.1",
  "description": "Best Copoun Finder",
  "permissions": ["activeTab", "tabs"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "firebase.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["app.js"],
      "css": ["coupon.css"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3
}
